{
  "name": "cpanel",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "start": "remix-serve ./build/index.js",
    "start:with_seed": "npm run seed:dev && npm run start",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "migrate:dev": "npx prisma migrate dev --name initialize_db",
    "seed:dev": "npx prisma db seed",
    "reinitialize:dev": "rm -rf ./prisma/migrations && rm ./prisma/database.db && npm run migrate:dev"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/roboto": "^5.0.12",
    "@headlessui/react": "^1.7.18",
    "@prisma/client": "^5.10.2",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/serve": "^2.8.1",
    "@tabler/icons-react": "^2.47.0",
    "classnames": "^2.5.1",
    "ioredis": "^5.3.2",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "slate": "^0.102.0",
    "slate-react": "^0.102.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/testing": "^2.8.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "daisyui": "^4.7.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jsdom": "^24.0.0",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
